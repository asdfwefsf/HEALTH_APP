<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/myappicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/group"
        android:supportsRtl="true"
        android:theme="@style/Theme.HEALTH_APP"
        tools:targetApi="33" >
        <activity
            android:name=".difault.DefaultActivity"
            android:exported="false"
            android:label="@string/title_activity_default"
            android:theme="@style/Theme.HEALTH_APP" />
        <activity
            android:name=".ExcerciseAddActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.HEALTH_APP" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>